<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.text.*?>

<AnchorPane prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="HospitalManagementSystem.controller.RegisterInfoController">
   <FlowPane alignment="CENTER" layoutY="50.0" prefHeight="300.0" prefWidth="300.0">
      <Label text="病人信息">
         <font>
            <Font size="24.0" />
         </font>
      </Label>
      <HBox alignment="CENTER" prefHeight="60.0" prefWidth="220.0">
         <Label alignment="CENTER" prefWidth="56.0" text="姓名">
            <font>
               <Font size="14.0" />
            </font>
         </Label>
         <TextField fx:id="fieldPatientName" prefHeight="31.0" prefWidth="161.0" />
      </HBox>
      <HBox alignment="CENTER" prefHeight="60.0" prefWidth="220.0">
         <Label alignment="CENTER" prefWidth="56.0" text="性别">
            <font>
               <Font size="14.0" />
            </font>
         </Label>
         <TextField fx:id="fieldPatientSex" prefHeight="31.0" prefWidth="161.0" />
      </HBox>
      <HBox alignment="CENTER" prefHeight="60.0" prefWidth="220.0">
         <Label alignment="CENTER" prefWidth="56.0" text="年龄">
            <font>
               <Font size="14.0" />
            </font>
         </Label>
         <TextField fx:id="fieldPatientAge" prefHeight="31.0" prefWidth="161.0" />
      </HBox>
      <HBox alignment="CENTER" prefHeight="60.0" prefWidth="220.0">
         <Label text="联系方式">
            <font>
               <Font size="14.0" />
            </font>
         </Label>
         <TextField fx:id="fieldPatientPhone" prefHeight="31.0" prefWidth="161.0" />
      </HBox>
   </FlowPane>
   <FlowPane alignment="CENTER" layoutX="300.0" layoutY="50.0" prefHeight="300.0" prefWidth="300.0">
      <Label text="医师信息">
         <font>
            <Font size="24.0" />
         </font>
      </Label>
      <HBox alignment="CENTER" prefHeight="60.0" prefWidth="220.0">
         <Label alignment="CENTER" prefWidth="56.0" text="姓名">
            <font>
               <Font size="14.0" />
            </font>
         </Label>
         <TextField fx:id="fieldDoctorName" prefHeight="31.0" prefWidth="161.0" />
      </HBox>
      <HBox alignment="CENTER" prefHeight="60.0" prefWidth="220.0">
         <Label alignment="CENTER" prefWidth="56.0" text="科室">
            <font>
               <Font size="14.0" />
            </font>
         </Label>
         <TextField fx:id="fieldDoctorDepartment" prefHeight="31.0" prefWidth="161.0" />
      </HBox>
      <HBox alignment="CENTER" prefHeight="60.0" prefWidth="220.0">
         <Label alignment="CENTER" prefWidth="56.0" text="职务">
            <font>
               <Font size="14.0" />
            </font>
         </Label>
         <TextField fx:id="fieldDoctorJob" prefHeight="31.0" prefWidth="161.0" />
      </HBox>
      <HBox alignment="CENTER" prefHeight="60.0" prefWidth="220.0">
         <Label alignment="CENTER" prefWidth="56.0" text="挂号费">
            <font>
               <Font size="14.0" />
            </font>
         </Label>
         <TextField fx:id="fieldRegisterFee" prefHeight="31.0" prefWidth="161.0" />
      </HBox>
   </FlowPane>
   <Button fx:id="btnReturn" layoutX="180.0" layoutY="352.0" mnemonicParsing="false" onAction="#onClickReturn" prefWidth="120.0" text="返回" textFill="#0810e2">
      <font>
         <Font size="24.0" />
      </font>
   </Button>
   <Button layoutX="300.0" layoutY="352.0" mnemonicParsing="false" prefWidth="120.0" text="打印" textFill="#0810e2">
      <font>
         <Font size="24.0" />
      </font>
   </Button>
   <Label layoutX="30.0" layoutY="20.0" text="编号：">
      <font>
         <Font size="18.0" />
      </font>
   </Label>
   <Label fx:id="labRegisterNum" layoutX="84.0" layoutY="16.0" text="Label" textFill="#ee0c0c">
      <font>
         <Font size="24.0" />
      </font>
   </Label>
   <Label layoutX="150.0" layoutY="20.0" text="挂号时间：">
      <font>
         <Font size="18.0" />
      </font>
   </Label>
   <Label fx:id="labRegisterTime" layoutX="240.0" layoutY="23.0" text="Label" textFill="#1d08c1">
      <font>
         <Font size="13.0" />
      </font>
   </Label>
   <Label layoutX="405.0" layoutY="20.0" text="专家号：">
      <font>
         <Font size="18.0" />
      </font>
   </Label>
   <Label fx:id="labIsExpert" layoutX="477.0" layoutY="16.0" text="Label">
      <font>
         <Font size="24.0" />
      </font>
   </Label>
</AnchorPane>
